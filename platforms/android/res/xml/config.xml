<?xml version='1.0' encoding='utf-8'?>
<widget id="pro.rudloff.stumbler" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="CellLocation">
        <param name="android-package" value="pro.rudloff.cordova.plugins.CellLocation" />
    </feature>
    <feature name="BackgroundMode">
        <param name="android-package" value="de.appplant.cordova.plugin.background.BackgroundMode" />
    </feature>
    <preference name="KeepRunning" value="true" />
    <feature name="Device">
        <param name="android-package" value="org.apache.cordova.device.Device" />
    </feature>
    <name>OpenCellStumbler</name>
    <description>
        Open source cell ID stumbler
    </description>
    <author email="contact@rudloff.pro" href="https://rudloff.pro/">
        Pierre Rudloff
    </author>
    <content src="index.html" />
    <access origin="*" />
</widget>
